    "react-dom": "^18.3.1",
    "remarkable": "^2.0.1",
    "tailwind-merge": "^2.3.0",
    "tsx": "^4.15.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.4.0",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5"
  },
  "name": "proff-quiz-bot",
