{
  "type": "module",
  "scripts": {
    "build": "tsc",
    "tsx": "tsx --env-file=.env ./app/bot.ts ",
    "dev": "tsx --watch --trace-warnings --env-file=.env.dev ./app/bot.ts",
    "start": "tsc && node --env-file=.env ./dist/app/bot.js",
    "docker_node": "node --trace-deprecation ./dist/app/bot.js",
    "dev_old": "node --watch --trace-warnings --env-file=.env --loader=ts-node/esm ./app/bot.ts"
  },
  "dependencies": {
    "@types/remarkable": "^2.0.8",
    "clsx": "^2.1.1",
