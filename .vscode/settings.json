{
  "cSpell.ignoreWords": [
    "Ubtrb",
    "conv",
    "okaidia",
    "ponicode",
    "prismjs"
  ],
  "explorer.fileNesting.patterns": {
    "*.ts": "$(capture).js, $(capture).*.ts",
    "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
    "*.jsx": "$(capture).js",
    "*.tsx": "$(capture).ts",
    "package.json": ".*, package-lock.json, tsconfig.json, tailwind.config.js, dockerfile, docker-compose.yml, Makefile"
  },
  "cSpell.words": [
    "clsx",
    "Consolas",
    "Fira",
    "paren"
  ],
}
